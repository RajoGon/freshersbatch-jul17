<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns:context="http://www.springframework.org/schema/context"
xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-4.2.xsd">

	<!-- Enable Annotation based configuration -->
	<context:annotation-config />


	<bean id="artRoom" class="com.springtest.ChatRoom">
	<property name="name" value="Art"/>
		<property name="users" >
			<set>	
				<value>G</value>
				<value>H</value>
				<value>I</value>
			</set>
		</property>
		<property name="messages">
			<list>
				<value>arts</value>
				<value>Hiart</value>
				<value>inart</value>
			</list>
		</property>
	</bean>
	<bean id="musicRoom" class="com.springtest.ChatRoom">
	<property name="name" value="Music"/>
		<property name="users" >
			<set>	
				<value>D</value>
				<value>E</value>
				<value>F</value>
			</set>
		</property>
		<property name="messages">
			<list>
				<value>Hello</value>
				<value>Hi</value>
				<value>hmmm</value>
			</list>
		</property>
	</bean>
	<bean id="sportsRoom" class="com.springtest.ChatRoom">
	<property name="name" value="Sports"/>
		<property name="users" >
			<set>	
				<value>A</value>
				<value>B</value>
				<value>C</value>
			</set>
		</property>
		<property name="messages">
			<list>
				<value>footbal</value>
				<value>cricket</value>
				<value>hockey</value>
			</list>
		</property>
	</bean>


</beans>



