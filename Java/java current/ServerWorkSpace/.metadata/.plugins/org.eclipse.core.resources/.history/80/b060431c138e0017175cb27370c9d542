package com.springtest.hibernate.entity.onetomany;
@Entity
@Table(name="MARKET")
public class Market {
	private long id;
	private String city;
	private String name;
	public Market(){}
	public Market(String city, String name) {
		super();
		this.city = city;
		this.name = name;
	}
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
}
