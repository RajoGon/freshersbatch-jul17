package com.annotation.student;
import com.annotation.getter.*;
public class Student {
	private int marks;
	private String name;
	
	public Student(){
		this.marks = 90;
		this.name = "Rajo";
	}
	@Getter(message = "Hello")
	public int getMarks(){
		return this.marks;
	}
	public String getName(){
		return this.name;
	}
}
