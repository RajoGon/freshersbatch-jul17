package com.enumtest;

enum Status { 
	NEW(1),REJECTED(2), ACCEPTED(3), COMPLETED(4);
		int value;
		Status(int value){
			this.value = value;
		}
	};
class OrderStatus{
	Status status;
	int orderid;
	String ordername;
	int price;
	
	
}
